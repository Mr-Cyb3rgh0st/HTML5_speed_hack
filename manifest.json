{
  "manifest_version": 3,
  "name": "Universal HTML5 Speed Hack",
  "version": "4.0",
  "author": "Mr.Cyb3rgh0$t",
  "description": "Lets you change the speed of any website.",
  "permissions": [
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {},
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["*://*/*"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
